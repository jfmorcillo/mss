#: .description .desc
msgid "Network File System service"
msgstr "Service de partage de fichiers sur le réseau"

#: .name
msgid "NFS service"
msgstr "Service NFS"

#: .packages[0].name
msgid "all"
msgstr "all"

#: .config[0].help
msgid ""
"Configure the firewall to allow access to the NFS service from internal "
"networks"
msgstr ""
"Configure le firewall pour autoriser l'accès au service NFS depuis les "
"réseaux internes"

#: .config[0].name
msgid "fw_lan"
msgstr "fw_lan"

#: .config[0].label
msgid "Allow access from internal networks"
msgstr "Autoriser l'accès depuis les réseaux internes"

#: .config[1].help
msgid ""
"Configure the firewall to allow access to the NFS service from external "
"networks"
msgstr ""
"Configure le firewall pour autoriser l'accès au service NFS depuis les "
"réseaux externes"

#: .config[1].name
msgid "fw_wan"
msgstr "fw_wan"

#: .config[1].label
msgid "Allow access from external networks"
msgstr "Autoriser l'accès depuis les réseaux externes"

#: nfs_server/setup-nfs-server.sh:81
msgid "The NFS service is running."
msgstr "Le service NFS est configuré."

#: nfs_server/setup-nfs-server.sh:82
msgid "Edit '/etc/exports' to configure your NFS shares"
msgstr "Gérez vos partages NFS dans le fichier '/etc/exports'"
